{
  "name": "creator",
  "version": "VI-alpha",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:wasm": "wasm-pack build --target web --dev --out-dir ../src/core/assembler/creatorAssembler/web/wasm/ CreatorCompiler && cd CreatorCompiler && deno run -A jsr:@deno/wasmbuild@0.19.3 --out ../src/core/assembler/creatorAssembler/deno/wasm/ --skip-opt",
    "wasm:web": "wasm-pack build --target web --out-dir ../src/core/assembler/creatorAssembler/web/wasm/ CreatorCompiler",
    "wasm:cli": "cd CreatorCompiler && deno run -A jsr:@deno/wasmbuild@0.19.3 --out ../src/core/assembler/creatorAssembler/deno/wasm/",
    "dev:web": "vite",
    "dev:cli": "deno run --allow-all --watch src/cli/creator6.mts",
    "build:gateway": "cd creatorDriver/esp32c3 && zip -9rq ../../gateway/esp32c3.zip . -x build/\\* && cd ../../gateway && cp esp32c3.zip esp32c6.zip && cp esp32c3.zip esp32h2.zip",
    "build:web": "bun run build:gateway && bun run wasm:web && vite build",
    "build:cli:native": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli ./src/cli/creator6.mts",
    "build:cli:windows": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli-windows --target x86_64-pc-windows-msvc ./src/cli/creator6.mts",
    "build:cli:mac-x64": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli-mac-x64 --target x86_64-apple-darwin ./src/cli/creator6.mts",
    "build:cli:mac-arm": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli-mac-arm --target aarch64-apple-darwin ./src/cli/creator6.mts",
    "build:cli:linux-x64": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli-linux-x64 --target x86_64-unknown-linux-gnu ./src/cli/creator6.mts",
    "build:cli:linux-arm": "deno compile --no-check -A --include ./src/core/assembler/creatorAssembler/deno -o creator-cli-linux-arm --target aarch64-unknown-linux-gnu ./src/cli/creator6.mts",
    "build:cli": "bun run wasm:cli && bun run build:cli:native",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "format": "prettier --write"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/legacy-modes": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@replit/codemirror-vim": "^6.3.0",
    "@types/humanize-duration": "^3.27.4",
    "@uiw/codemirror-themes": "^4.25.1",
    "ansicolor": "^2.0.3",
    "apexcharts": "^5.3.5",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.40.6",
    "codemirror": "^6.0.2",
    "colors": "^1.4.0",
    "deno": "^2.5.0",
    "humanize-duration": "^3.33.0",
    "js-yaml": "^4.1.0",
    "qrcode.vue": "^3.6.0",
    "readline-sync": "^1.4.10",
    "vite-node": "^3.2.4",
    "vite-plugin-wasm": "^3.5.0",
    "vue": "^3.5.21",
    "vue-codemirror6": "^1.3.22",
    "vue3-apexcharts": "^1.8.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.9",
    "@tsconfig/node22": "^22.0.2",
    "@types/colors": "^1.2.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.1",
    "@types/yargs": "^17.0.33",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.92.1",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "typescript": "~5.9.2",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.5",
    "vite-multiple-assets": "^2.2.5",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.7"
  },
  "bin": {
    "creator": "./dist/cli/creator.js"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "deno",
    "esbuild",
    "sharp",
    "vue-demi"
  ]
}
