{
  "name": "creator",
  "version": "6.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:wasm": "wasm-pack build --target web --dev --out-dir ../src/core/assembler/creatorAssembler/web/wasm/ creator-assembler && cd creator-assembler && deno run -A jsr:@deno/wasmbuild@0.19.3 --out ../src/core/assembler/creatorAssembler/deno/wasm/ --skip-opt",
    "wasm:web": "wasm-pack build --target web --out-dir ../src/core/assembler/creatorAssembler/web/wasm/ creator-assembler",
    "wasm:cli": "cd creator-assembler && deno run -A jsr:@deno/wasmbuild@0.19.3 --out ../src/core/assembler/creatorAssembler/deno/wasm/",
    "dev:web": "vite",
    "dev:cli": "deno run --allow-all --watch src/cli/creator-cli.mts",
    "build:web": "bun run wasm:web && vite build",
    "build:cli:native": "./scripts/build-cli.sh -o creator-cli",
    "build:cli:windows": "./scripts/build-cli.sh -o creator-cli-windows -t x86_64-pc-windows-msvc",
    "build:cli:mac-x64": "./scripts/build-cli.sh -o creator-cli-mac-x64 -t x86_64-apple-darwin",
    "build:cli:mac-arm": "./scripts/build-cli.sh -o creator-cli-mac-arm -t aarch64-apple-darwin",
    "build:cli:linux-x64": "./scripts/build-cli.sh -o creator-cli-linux-x64 -t x86_64-unknown-linux-gnu",
    "build:cli:linux-arm": "./scripts/build-cli.sh -o creator-cli-linux-arm -t aarch64-unknown-linux-gnu",
    "build:cli": "bun run wasm:cli && bun run build:cli:native",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "format": "prettier --write"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/legacy-modes": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@replit/codemirror-vim": "^6.3.0",
    "@types/humanize-duration": "^3.27.4",
    "@uiw/codemirror-themes": "^4.25.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "ajv": "^8.17.1",
    "ansicolor": "^2.0.3",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.40.6",
    "codemirror": "^6.0.2",
    "codemirror-json-schema": "^0.8.1",
    "colors": "^1.4.0",
    "deno": "^2.5.4",
    "humanize-duration": "^3.33.0",
    "js-yaml": "^4.1.0",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.54.0",
    "monaco-vim": "^0.4.2",
    "monaco-yaml": "^5.4.0",
    "qrcode.vue": "^3.6.0",
    "readline-sync": "^1.4.10",
    "vite-node": "^3.2.4",
    "vite-plugin-wasm": "^3.5.0",
    "vue": "^3.5.21",
    "vue-codemirror6": "^1.4.1",
    "xterm": "^5.3.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.9",
    "@prettier/plugin-pug": "^3.4.2",
    "@tsconfig/node22": "^22.0.5",
    "@types/colors": "^1.2.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/yargs": "^17.0.35",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.3",
    "prettier-plugin-vue": "^1.1.6",
    "sass-embedded": "^1.93.3",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "typescript": "~5.9.3",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-multiple-assets": "^2.2.6",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.4",
    "wasm-pack": "^0.13.1"
  },
  "bin": {
    "creator": "./dist/cli/creator.js"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "deno",
    "esbuild",
    "sharp",
    "vue-demi",
    "wasm-pack"
  ]
}
